<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wuhenjian.microservicescaffolding.service.dao.DemoDao" >

  <insert id="insert" parameterType="com.wuhenjian.microservicescaffolding.service.domain.entity.DemoEntity">
    <selectKey resultType="java.lang.Long" keyProperty="id" order="AFTER" >
      SELECT LAST_INSERT_ID()
    </selectKey>
    INSERT INTO demo
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null">
        `id`,
      </if>
      <if test="demo != null and demo != ''">
        `demo`,
      </if>
    </trim>
    <trim prefix="VALUES (" suffix=")" suffixOverrides="," >
      <if test="id != null">
        #{id},
      </if>
      <if test="demo != null and demo != ''">
        #{demo},
      </if>
    </trim>
  </insert>
  <select id="get" resultType="com.wuhenjian.microservicescaffolding.service.domain.dto.DemoDTO">
    SELECT `demo` FROM demo WHERE `demo` = #{demo}
  </select>
</mapper>